<%  if(subscriptionListLength > 0) {%>
    <rn:block id="preList"/>
    <% for (var i = 0; i < subscriptionListLength; i++) { %>
    <rn:block id="preItem"/>
        <div class="rn_Notification" id="<%= widgetInstanceID %>_<%= subscriptionList.items[i].subscriptionID %>">
            <rn:block id="preInfo"/>
            <div class="rn_Notification_Info">
                <rn:block id="subscriptionName">
                    <span><%= subscriptionList.items[i].name %></span>
                </rn:block>
                <rn:block id="subscriptionDetail">
                    <% if(subscriptionList.items[i].subscriptionType === 'SUBSCRIPTIONTYPE_CHANNEL') { %>
                        <% var prodCat = getCategories(subscriptionList.items[i]); %>
                        <% var subscriptionDetail = labelContentType + ' - ' + subscriptionList.items[i].contentType.name; %>
                        <% if(prodCat['product']) { %>
                            <% if(enableProdCategHyperlink) { %>
                                <% subscriptionDetail += ', ' + labelProduct + ' - <a href="' + prodCat['phref'] + '" target="' + target + '">' + prodCat['product'] + '</a>' %>
                            <% } else {%>
                                <% subscriptionDetail += ', ' + labelProduct + ' - ' + prodCat['product']; %>
                            <% } %>
                        <% } %>
                        <% if(prodCat['category']) { %>
                            <% if(enableProdCategHyperlink) { %>
                                <% subscriptionDetail += ', ' + labelCategory + ' - <a href="' + prodCat['chref'] + '" target="' + target + '">' + prodCat['category'] + '</a>' %>
                            <% } else {%>
                                <% subscriptionDetail += ', ' + labelCategory + ' - ' + prodCat['category']; %>
                            <% } %>
                        <% } %>
                        <span><%= subscriptionDetail %></span>
                    <% } %>
                </rn:block>
                <rn:block id="startDate">
                    <span><%= subscriptionList.items[i].expires %></span>
                </rn:block>
                <rn:block id="expiration">
                    <span><%= subscriptionList.items[i].expiresTime %></span>
                </rn:block>
            </div>
            <rn:block id="postInfo"/>
            <rn:block id="preActions"/>
            <div class="rn_Notification_Actions">
                <rn:block id="preDelete"/>
                <button id="<%= subscriptionList.items[i].subscriptionID %>" class="rn_Notification_Delete"><%= labelDeleteButton %></button>
                <rn:block id="postDelete"/>
            </div>
            <rn:block id="postActions"/>
        </div>
        <rn:block id="postItem"/>
    <% } %>
    <rn:block id="postList"/>
<% } %>